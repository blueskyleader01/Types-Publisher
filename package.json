  "dependencies": {
    "adal-node": "^0.1.22",
    "azure-keyvault": "^0.11.0",
    "azure-storage": "1.2.0",
    "fs-promise": "^0.5.0",
    "fstream": "^1.0.10",
    "longjohn": "^0.2.11",
    "moment": "^2.13.0",
    "node-fetch": "^1.5.3",
    "nodegit": "^0.16.0",
    "npm": "^3.10.7",
    "npm-registry-client": "^7.1.2",
    "recursive-readdir": "^2.0.0",
    "source-map-support": "^0.4.0",
    "tar": "^2.2.1",
