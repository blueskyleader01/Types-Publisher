  "dependencies": {
    "azure-storage": "^1.1.0",
    "buffer-equals-constant": "^1.0.0",
    "fs-promise": "^0.5.0",
    "moment": "^2.13.0",
    "node-fetch": "^1.5.3",
    "recursive-readdir": "^2.0.0",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0",
    "yargs": "^4.7.1"
  },
